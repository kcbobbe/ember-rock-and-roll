
<div class="container">
  <div class="pb-2 mt-4 mb-2 border-bottom">
    <h1>Rock and Roll<small> with Ember.js</small></h1>
  </div>
  <div class="row">
    <div class="col-md-12">
      <ul class="list-group songs">
        {{#each model as |song|}}
          <li class="list-group-item">
            {{song.title}} by <em>{{song.band}}</em>
            <span class="rating float-right">{{song.rating}}</span>
        </li>
        {{/each}}
      </ul>
    </div>
  </div>
</div>